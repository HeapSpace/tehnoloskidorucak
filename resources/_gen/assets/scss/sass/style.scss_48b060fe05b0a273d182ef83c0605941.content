html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;font-size:100%;font:inherit;vertical-align:baseline}article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}body{line-height:1}ol,ul{list-style:none}blockquote,q{quotes:none}blockquote:before,blockquote:after,q:before,q:after{content:'';content:none}table{border-collapse:collapse;border-spacing:0}@font-face{font-family:hs grotesk;src:url("/fonts/HS Grotesk.otf") format("opentype");font-weight:400;font-style:normal}@font-face{font-family:menoe grotesque;src:url(/fonts/Menoe-Grotesque-Std-Regular.otf) format("opentype");font-weight:400;font-style:normal}html{font-family:menoe grotesque,sans-serif;-ms-text-size-adjust:100%;font-size:100%;-webkit-text-size-adjust:100%;font-variant-ligatures:none;-webkit-font-variant-ligatures:none;text-rendering:optimizeLegibility;-moz-osx-font-smoothing:grayscale;-webkit-font-smoothing:antialiased;text-shadow:rgba(0,0,0,.01) 0 0 1px}.red{color:#fd4000}.beige{color:#fff4e1}.blue{color:#384af4}.black{color:#231f20}.white{color:#fff}.bg-red{background-color:#fd4000;color:#fff}.bg-beige{background-color:#fff4e1;color:#fd4000}.bg-blue{background-color:#384af4;color:#fff}.bg-black{background-color:#231f20;color:#fff}.bg-white{background-color:#fff;color:#231f20}.bg-white a{color:#231f20;pointer-events:all}h1,h2,h3,h4,h5,h6{font-family:hs grotesk,sans-serif;font-weight:400;clear:both;padding:0;margin-top:0}h1{font-size:6em}h2{font-size:2em}h3{font-size:1.4em;margin-bottom:.7em}p{margin-bottom:1.5em}a{color:#fff}a:visited{opacity:.6}a:hover,a:focus,a:active{color:inherit;text-decoration:none}a:focus{outline:thin dotted}a:hover,a:active{outline:0}hr{background-color:#ccc;border:0;height:1px;margin-bottom:1.5em}img{height:auto;max-width:100%;border:0}.align-left{display:inline;float:left;margin-right:1.5em}.align-right{display:inline;float:right;margin-left:1.5em}.align-center{clear:both;display:block;margin-left:auto;margin-right:auto}.clear:before,.clear:after{content:"";display:table;table-layout:fixed}.clear:after{clear:both}button,input{overflow:visible}button,input,optgroup,select,textarea{font-size:100%;line-height:1.15;margin:0;font-family:menoe grotesque,sans-serif}ul{margin:40px 0}.grid{display:grid;grid-template-columns:1fr minmax(auto,1120px) 1fr;grid-gap:1em;padding:4em 0}.content-wrap{grid-column:2;font-size:2em;line-height:1.2}.boxes{display:grid;grid-template-columns:33% 33% 33%;grid-gap:10px;margin:6em 0 3em}.box{border-radius:5px;padding:20px;position:relative}h1#title{font-size:4em;line-height:.9em;margin-top:1em;margin-bottom:1em}h2.town{white-space:nowrap}h3.date{margin-top:40px;margin-bottom:0}ul.talks{margin:2em 0}ul.talks li{font-size:.7em;margin-top:20px;letter-spacing:-2px}ul.talks li span{font-size:.8em;position:relative;top:0;margin-right:10px}div.meetup{position:absolute;bottom:10px}footer div#footer{font-size:.7em;margin:2em 0 0}footer ul{list-style:none;padding-left:0;margin-left:0}#mc_embed_signup{clear:left;font-size:.7em;width:100%}#mc_embed_signup input.email{border:1px solid #231f20;color:#231f20;background-color:transparent;box-shadow:none;-moz-box-shadow:none;-webkit-box-shadow:none;-webkit-appearance:none;-webkit-border-radius:0;padding:30px 20px;display:inline-block;margin:0;z-index:2;width:80%;float:left}#mc_embed_signup .button{height:84px;width:84px;line-height:88px;border-radius:100%;background-color:#231f20;box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;display:inline-block;cursor:pointer;color:#fff;margin-left:20px;margin-top:0;font-size:50px;z-index:2;float:left}@media screen and (max-width:1000px){.boxes{display:grid;grid-template-columns:100%;margin-top:3em}h1#title{margin:.6em 0}h3.date{margin:0}ul.talks{margin:1em 0 2em}}@media screen and (max-width:768px){body{font-size:.9em}.boxes{margin:2em 0}}@media screen and (max-width:600px){body{font-size:.8em}.content-wrap{padding:0}h1#title{font-size:2em;margin:.5em 0}p{font-size:.8em;line-height:1.1em}}